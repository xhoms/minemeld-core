{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "minemeld.ft.csv.CSVFT",
    "title": "mminemeld.ft.csv.CSVFT static configuration",
    "type": "object",
    "$extends": {
        "base": {
            "$ref": "minemeld.ft.basepoller.BasePollerFT#"
        }
    },
    "properties": {
        "verify_cert": {
            "type": "boolean"
        },
        "polling_timeout": {
            "type": "integer"
        },
        "delimiter": {
            "type": "string",
            "maxLength": 1,
            "minLength": 1
        },
        "doublequote": {
            "type": "boolean"
        },
        "escapechar": {
            "type": "string",
            "maxLength": 1,
            "minLength": 1
        },
        "quotechar": {
            "type": "string",
            "maxLength": 1,
            "minLength": 1
        },
        "skipinitialspace": {
            "type": "boolean"
        },
        "url": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "password": {
            "type": "string"
        },
        "ignore_regex": {
            "type": "string"
        },
        "fieldnames": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "url"
    ],
    "additionalProperties": false
}